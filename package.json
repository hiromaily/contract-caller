{
  "name": "contract-caller",
  "version": "1.0.0",
  "description": "contract caller",
  "license": "MIT",
  "scripts": {
    "build": "truffle compile",
    "fmt": "prettier --write '{src,test}/**/*.{ts,js}'",
    "lint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "type-check": "tsc --noEmit",
    "call": "ts-node src/call.ts --mode default",
    "call2": "node --loader ts-node/esm src/call.ts"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.1",
    "solc": "^0.8.17",
    "truffle": "^5.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "commander": "^9.4.1",
    "dotenv": "^16.0.3",
    "web3": "^1.8.1",
    "web3-utils": "^1.8.1"
  }
}
